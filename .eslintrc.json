{
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true,
		"jest": true,
		"amd": true
	},
	"plugins": [
		"react",
		"prettier"
	],
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"prettier"
	],
	"rules": {
		"prettier/prettier": [
			"error",
			{},
			{
				"usePrettierrc": true
			}
		],
		"linebreak-style": "off",
		"react/prop-types": "off"
	}
}