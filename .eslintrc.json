{
	"parserOptions": {
		"ecmaVersion": 2017,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"node": true,
		"es6": true,
		"jest": true,
		"amd": true
	},
	"plugins": ["react", "prettier"],
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"prettier"
	],
	"rules": {
		"prettier/prettier": [
				"error",
				{
						"endOfLine": "auto"
				}
		],
		"linebreak-style": 0,
		"react/prop-types": 0
	}
}
